<?xml version="1.0" encoding="UTF-8"?>
<ehcache name="passwordcache">

    <diskStore path="java.io.tmpdir"/>

    <cache name="passwordRetryCache"
           maxEntriesLocalHeap="2000"
           eternal="false"
           timeToIdleSeconds="10"
           timeToLiveSeconds="0"
           overflowToDisk="false"
           statistics="true">
    </cache>

</ehcache>